   ],
   "source": [
    "spo2 = 90\n",
    "age = 50\n",
    "hb = 11.5\n",
    "w =75\n",
    "lc = 75\n",
    "print(\"SPO2:\",spo2)\n",
    "print(\"Age:\",age)\n",
    "print(\"Hemo\",hb)\n",
    "print(\"weight\",w)\n",
    "print(\"lc:\",lc)\n",
    "s = ((2*spo2)+age+(4*hb)+w+(lc*lc))\n",
    "print(\"Risk Factor before dividing:\",s)\n",
    "s = s/8000\n",
    "print(\"Risk Factor\",s)"
   ]
